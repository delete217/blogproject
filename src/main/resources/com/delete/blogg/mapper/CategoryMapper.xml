<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.delete.blogg.mapper.CategoryMapper">
    <select id="findCategoryById" parameterType="long" resultType="com.delete.blogg.entity.Category">
        SELECT * FROM `ms_category` WHERE id = #{categoryId}
    </select>
</mapper>
